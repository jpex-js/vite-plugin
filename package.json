{
  "name": "@jpex-js/vite-plugin",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Jack Ellis",
  "license": "MIT",
  "scripts": {
    "test": "echo \"no tests\"",
    "test:debug": "ava debug",
    "lint": "eslint './src/**/*.ts' && tsc --noEmit",
    "build:prepare": "rm -rf dist",
    "build:js": "rollup --config ./rollup.config.js",
    "build:ts": "tsc -d --outDir dist --emitDeclarationOnly --downlevelIteration ./src/index.ts",
    "build": "yarn build:prepare && yarn build:js && yarn build:ts",
    "prepublishOnly": "yarn build",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jpex-js/vite-plugin.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --edit"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{js,ts}": [
        "eslint -c ./.eslintrc.js",
        "git add"
      ]
    }
  },
  "peerDependencies": {
    "@babel/core": "^7.11.4",
    "jpex": "^4.3.3",
    "vite": "^2.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/helper-plugin-utils": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/config-conventional": "^9.1.2",
    "@team-griffin/eslint-config": "^3.3.0",
    "@types/babel__core": "^7.1.14",
    "@types/node": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.7.0",
    "jpex": "^4.3.3",
    "rollup": "^2.26.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "semantic-release": "^17.1.1",
    "typescript": "^4.0.2",
    "vite": "^2.3.4"
  },
  "dependencies": {
    "@babel/plugin-syntax-typescript": "^7.12.13"
  }
}
